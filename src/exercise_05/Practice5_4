package exercise;

public class Practice5_4 {
	public static void main(String[] args) {
		int n = 200;
		System.out.println("全部で" + n + "人です");
		
		double a = 11.5;
		double b = 3.0;
		double answer = a + (2.0 * b);
		System.out.println("答えは=" + answer);
		
		int gaku = 1200;
		int zei = 120;
		System.out.println("税込で" + (gaku + zei) + "円です");
	}

}
